Namespace: redpoint.games
Rulesets:
- Name: oss-v1-rules
  Severity: Error
  Rules:
  - Name: no-unsafe-calls
Rules:
- Name: no-unsafe-calls
  Matcher: |
    cxxMemberCallExpr(
      on(hasType(qualType(pointsTo(namedDecl(matchesName("IOnline.+")))))),
      callee(functionDecl(returns(asString("_Bool"))))
    ).bind("call")
  ErrorMessage: |
    unsafe OSSv1 call detected; please call via FSafeOSSv1Caller instead
  Callsite: call